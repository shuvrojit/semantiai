{
  "name": "Semantiai",
  "description": "Your personal assistant for every website",
  "manifest_version": 3,
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Semantiai extension"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "contextMenus"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}
